{
	"name": "prettier-plugin-esjs",
	"version": "0.1.0-beta.2",
	"license": "MIT",
	"type": "module",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pkgroll",
		"start": "tsx src/index.ts",
		"test": "vitest",
		"test:run": "vitest run",
		"typecheck": "tsc --noEmit",
		"watch": "pkgroll --watch",
		"prepublishOnly": "pnpm run build"
	},
	"devDependencies": {
		"@es-js/core": "workspace:*",
		"@prettier/sync": "^0.5.5",
		"@types/node": "^20.19.30",
		"pkgroll": "^2.22.0",
		"prettier": "^3.8.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite": "^5.4.21",
		"vitest": "^2.1.9"
	},
	"peerDependencies": {
		"prettier": "^3.0.0"
	}
}
