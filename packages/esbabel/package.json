{
  "name": "@es-js/esbabel",
  "version": "0.0.1-beta.1",
  "type": "module",
  "scripts": {
    "build": "unbuild",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "esjs",
    "lenguaje-programacion",
    "espa√±ol",
    "spanish"
  ],
  "devDependencies": {
    "@types/node": "^20.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^5.1.1",
    "vitest": "^2.1.5"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
