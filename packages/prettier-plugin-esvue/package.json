{
	"name": "prettier-plugin-esvue",
	"version": "0.0.1-beta.1",
	"license": "MIT",
	"type": "module",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pkgroll",
		"start": "tsx src/index.ts",
		"test": "vitest",
		"test:run": "vitest run",
		"typecheck": "tsc --noEmit",
		"watch": "pkgroll --watch",
		"prepublishOnly": "pnpm run build"
	},
	"devDependencies": {
		"@es-js/core": "workspace:*",
		"@es-js/eshtml": "workspace:*",
		"@es-js/esvue": "workspace:*",
		"@prettier/sync": "^0.5.2",
		"@types/node": "^20.14.12",
		"pkgroll": "^2.4.1",
		"prettier": "^3.3.3",
		"tsx": "^4.16.2",
		"typescript": "^5.5.4",
		"vite": "^5.3.4",
		"vitest": "^2.0.4"
	},
	"peerDependencies": {
		"prettier": "^3.0.0"
	}
}
